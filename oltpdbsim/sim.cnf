simulation.name=static-hgr-random

workload.aware=true
workload.representation=hgr

incremental.repartitioning=false
static.repartitioning=false

transaction.classification.strategy=basic
data.migration.strategy=random

compression.enabled=false
compression.before.setup=false
sword.compression.ratio=6
sword.dt.threshold=0.5

initial.servers=2
server.capacity=10000
fixed.partitions=4
number.of.replicas=1

simulation.period=21600
inverse.of.mean.inter.arrival.time=1
inverse.of.mean.service.time=1000

workload.change.probability=0.5
dt.change.percentage=0.5
initial.workload.size=3600