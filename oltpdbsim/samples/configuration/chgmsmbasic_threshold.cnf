warmup.period=10800

inverse.of.mean.inter.arrival.time=1
inverse.of.mean.service.time=1000

percentage.change.in.workload=0.1
adjustment=0.4
observation.window.size=3600
unique.max.fixed=900
exponential.Avg.Weight=0.4

percentage.idt.threshold.inc=0.01

methodX.decision.priority=0.5