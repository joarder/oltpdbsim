simulation.name=sword

workload.aware=true
workload.representation=chg

incremental.repartitioning=true

transaction.classification.strategy=basic
data.migration.strategy=sword

compression.before.setup=true
sword.compression.ratio=6
sword.dt.threshold=0.5

initial.servers=4
server.capacity=10000
fixed.partitions=64
number.of.replicas=1

simulation.period=86400
inverse.of.mean.inter.arrival.time=1
inverse.of.mean.service.time=1000

workload.change.probability=0.5
dt.change.percentage=0.1
initial.workload.size=3600